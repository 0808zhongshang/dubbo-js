{
  "private": true,
  "name": "dubbo-js-private-workspace",
  "workspaces": [
    "docs",
    "packages/*"
  ],
  "scripts": {
    "build-lint": "cd ./lint && go build -o ../dj_lint",
    "build": "npx nx run-many --target=build",
    "test": "npx nx run-many --target=test",
    "test:watch": "npx nx run-many --target=test:watch",
    "ts-check": "npx nx run-many --target=ts-check",
    "format": "npx nx run-many --target=format",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@nrwl/cli": "14.8.3",
    "@nrwl/workspace": "14.8.3",
    "@types/node": "latest",
    "pretty-quick": "^1.10.0",
    "nx": "14.8.3",
    "prettier": "^2.6.2",
    "typescript": "~4.8.2",
    "vite": "^3.1.8",
    "vitest": "^0.24.3",
    "husky": "^6.0.0"
  },
  "license": "Apache-2.0"
}