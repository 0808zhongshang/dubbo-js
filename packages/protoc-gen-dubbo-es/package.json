{
  "name": "apache-protoc-gen-dubbo-es",
  "version": "0.0.4",
  "description": "Code generator for Dubbo",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/apache/dubbo-js.git",
    "directory": "packages/protoc-gen-dubbo-es"
  },
  "bin": {
    "protoc-gen-dubbo-es": "bin/protoc-gen-dubbo-es"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./dist/cjs/*",
    "build": "tsc --project tsconfig.json --module commonjs --outDir ./dist/cjs"
  },
  "preferUnplugged": true,
  "dependencies": {
    "@bufbuild/protobuf": "^1.2.1",
    "@bufbuild/protoplugin": "^1.2.1"
  },
  "peerDependencies": {
    "apache-dubbo": "^0.0.3",
    "@bufbuild/protoc-gen-es": "^1.2.1"
  },
  "peerDependenciesMeta": {
    "apache-dubbo": {
      "optional": true
    },
    "@bufbuild/protoc-gen-es": {
      "optional": true
    }
  },
  "files": [
    "dist/**"
  ]
}
